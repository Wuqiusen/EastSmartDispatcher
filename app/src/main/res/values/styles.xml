    <!--主界面顶部按钮样式-->
    <style name="main_activity_top_button">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/font_small</item>
        <item name="android:textColor">@color/font_blue</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_weight">0.2</item>
    </style>

    <!--主界面底部停班按钮样式-->
    <style name="main_activity_bottom_button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:gravity">center</item>
        <!--<item name="android:background">@color/tv_red</item>-->
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">20dp</item>
    </style>

    <style name="main_child_title_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">2dp</item>
        <item name="android:textSize">@dimen/font_tiny</item>
        <item name="android:textColor">@color/font_blue3</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentLeft">true</item>
    </style>

    <style name="stop_car_dialog_radio_button">
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:paddingLeft">6dp</item>
    </style>
    <style name="stop_car_dialog_show_tv" parent="stop_car_dialog_radio_button">
        <item name="android:textSize">@dimen/font_large</item>
    </style>
    <style name="stop_car_dialog_edittext">
        <item name="android:textSize">@dimen/font_large</item>
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:paddingLeft">6dp</item>
        <item name="android:maxLines">1</item>
        <item name="android:maxLength">10</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textCursorDrawable">@drawable/color_cursor</item>
